{
  "version": 2,
  "routes": [
    { "src": "/(.*)", "dest": "/" }
  ],
  "rewrites": [
    { 
      "source": "/api/(.*)", 
      "destination": "https://api.head2head.dev/api/$1" 
    }
  ],
  "redirects": [
    { 
      "source": "http://head2head.dev", 
      "destination": "https://head2head.dev", 
      "permanent": true 
    },
    { 
      "source": "http://www.head2head.dev", 
      "destination": "https://head2head.dev", 
      "permanent": true 
    }
  ]
}