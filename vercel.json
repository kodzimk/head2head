{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://api.head2head.dev/:path*"
    },
    {
      "source": "/avatars/:path*",
      "destination": "https://api.head2head.dev/avatars/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "/avatars/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}